<%- include("partials/header") %>

<h1>Home</h1>
<p><%= startingContent %></p>
<% posts.forEach((post) => { %>
<% let newTitle = post.title.split(" ").join("-") %>
<% let s = "http://localhost:3000/posts/" + newTitle%>
<h2><%= post.title %></h2>
<p><%= post.body %><a href=<%= s %>>Read More</a></p>
<% }); %>

<%- include("partials/footer") %>